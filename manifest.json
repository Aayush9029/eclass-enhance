{

  "description": "Makes eclass look better, subjectively.",
  "manifest_version": 2,
  "name": "eclass-enhance",
  "version": "1.0",
  "homepage_url": "https://github.com/Aayush9029/eclass-enhance",

  "background": {
    "scripts": ["background.js"]
  },

  "page_action": {
    "default_icon": "icons/on.svg",
    "browser_style": true
  },
  
  "permissions": [
    "tabs",
    "*://eclass.yorku.ca/*"
  ]

}
